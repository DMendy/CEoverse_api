{
  "info": {
    "name": "CEOverse Contact API",
    "description": "Collection Postman pour tester l'endpoint /api/contact.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.0.0"
  },
  "item": [
    {
      "name": "POST /api/contact",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Ava Exemple\",\n  \"email\": \"ava@example.com\",\n  \"message\": \"Bonjour CEOverse !\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/contact",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "contact"
          ]
        }
      },
      "response": []
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:4000"
    }
  ]
}
